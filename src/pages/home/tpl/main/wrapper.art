{{if data}}
<div id="{{data.contentId}}_wrapper" style="height:{{data.height}}px;position:relative">
    {{include './header.art' data}}
    <div class="content-wrapper" id="{{data.contentId}}">
    </div>
    <div class="no-data-wrapper hidden">
        <div class="no-data">
            <span class="mo-icons-bg warm-icon"></span>
            <span class="warm-text"></span>
        </div>
    </div>
</div>
{{/if}}
